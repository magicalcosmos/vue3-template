{
	"tabWidth": 2,
	"useTabs": false,
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"requirePragma": false,
	"proseWrap": "never",
	"vueIndentScriptAndStyle": true,
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "auto"
}
